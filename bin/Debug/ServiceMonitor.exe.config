<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
	<!-- Add services to monitor below. Line 9 contains an example format. Services can be on multiple different servers and/or computers. -->
	<!-- A key named "Refresh Interval" can be added with a value in milliseconds to change the frequency the program checks for the current status of services. -->
	<!-- <add key="Display name to show on screen" value="Windows Service Name,Server Name"/> -->
    <add key="[localhost] Print Spooler DisplayName" value="Print Spooler,localhost"/>
    <add key="[shavlikprd01] Print Spooler DisplayName" value="Print Spooler,shavlikprd01"/>
    <add key="App Readiness DisplayName" value="App Readiness,localhost"/>
	<add key="Refresh Interval" value="2000"/>
  </appSettings>
</configuration>